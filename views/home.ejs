<%- include("partials/header"); -%>


<h1>Home</h1>
<p> <%= homeStartingContent %> </p>


<% posts.forEach(createPost) %>
    
<% function createPost(post) { %>
    <h1> <%= post.title %> </h1>
    <p> <%= post.content.substring(0,100) + "..." %> <a href="/posts/<%= post.title %>" >Read More</a> </p>
<% } %>
    <button  class="btn btn-primary compose"><a href="/compose"> Compose Post </a></button>

<%- include("partials/footer"); -%>